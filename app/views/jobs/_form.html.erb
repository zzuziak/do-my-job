<%= simple_form_for job do |f| %>
<%= f.input :title %>
<%= f.input :category, collection: ["arts & design", "business & finance", "science", "academia", "sports & events", "services", "gastronomy", "entertainment", "transportation", "politics", "healthcare"] %>
<%= f.input :location %>
<%= f.input :description %>
<%= f.input :price %>
<%= f.input :guided, input_html: {value: :guided} %>
<%= f.input :date %>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>
<%= f.submit %>
<% end %>
